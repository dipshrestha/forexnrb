{
  "manifest_version": 2,

  "name": "Hello World  Chrome Plug-in",
  "description": "This extension demonstrates a browser action with kittens.",
  "version": "1.0",

  "permissions": ["http://www.nrb.org.np//*/"],
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
      "js": ["jquery-1.2.6.js", "jquery.xml2json.js"],
	  "matches":["*://*/*"],
	  "run_at":"document_start"
    }]
}